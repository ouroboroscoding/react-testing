{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"declaration": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"jsx": "react",
		"lib": ["es2017", "dom"],
		"types": ["react", "node"],
		"moduleResolution": "node",
		"noEmit": false,
		"outDir": ".",
		"resolveJsonModule": true,
		"strict": true,
		"strictPropertyInitialization": false,
		"target": "esnext"
	},
	"include": ["./src"],
	"exclude": []
}